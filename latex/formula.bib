  publisher = {IEEE},
  author    = {Onar, Omer C. and Miller, John M. and Campbell, Steven L. and Coomer, Chester and White, Cliff. P. and Seiber, Larry E.},
  month     = mar,
  year      = {2013},
  pages     = {3073--3080},
  file      = {Onar et al. - 2013 - A novel wireless power transfer for in-motion EVP.pdf:files/424/Onar et al. - 2013 - A novel wireless power transfer for in-motion EVP.pdf:application/pdf}
}

@article{rakhymbay_precise_2018,
  title     = {Precise {Analysis} on {Mutual} {Inductance} {Variation} in {Dynamic} {Wireless} {Charging} of {Electric} {Vehicle}},
  volume    = {11},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  url       = {http://www.mdpi.com/1996-1073/11/3/624},
  doi       = {10.3390/en11030624},
  abstract  = {Wireless power transfer provides an opportunity to charge electric vehicles (EVs) without electrical cables. Two categories of this technique are distinguished: Stationary Wireless Charging (SWC) and Dynamic Wireless Charging (DWC) systems. Implementation of DWC is more desirable than SWC as it can potentially eliminate challenges associated with heavy weight batteries and time-consuming charging processes. However, power transfer efficiency and range, lateral misalignment of coils as well as implementation cost are issues affecting DWC. These issues need to be addressed through developing coil architectures and topologies as well as operating novel semiconductor switches at higher frequencies. This study presents a small-scale dynamic wireless power transfer system for EV. It specifically concentrates on analyzing the dynamic mutual inductance between the coils due to the misalignment as it has significant influence on the EV charging process, particularly, over the output power and overall efficiency. A simulation study is carried out to explore dynamic mutual inductance profile between the transmitter and receiver coils. Mutual inductance simulation results are then verified through practical measurements on fabricated coils. Integrating the practical results into the model, an EV DWC power transfer simulation is conducted and the relation between dynamic mutual inductance and output power are discussed technically.},
  language  = {en},
  number    = {3},
  urldate   = {2018-09-18},
  journal   = {Energies},
  author    = {Rakhymbay, Ainur and Khamitov, Anvar and Bagheri, Mehdi and Alimkhanuly, Batyrbek and Lu, Maxim and Phung, Toan and Rakhymbay, Ainur and Khamitov, Anvar and Bagheri, Mehdi and Alimkhanuly, Batyrbek and Lu, Maxim and Phung, Toan},
  month     = mar,
  year      = {2018},
  keywords  = {coil design, Dynamic Wireless Power Transfer (DWPT), electric vehicle (EV), mutual inductance},
  pages     = {624},
  file      = {Snapshot:files/445/htm.html:text/html}
}

@article{karam_hwang_autonomous_2017,
  title    = {An {Autonomous} {Coil} {Alignment} {System} for the {DynamicWireless} {Charging} of {Electric} {Vehicles} to {Minimize} {Lateral} {Misalignment}},
  volume   = {10},
  issn     = {19961073},
  url      = {http://search.ebscohost.com/login.aspx?direct=true&db=asn&AN=122141949&site=ehost-live},
  doi      = {10.3390/en10030315},
  abstract = {This paper proposes an autonomous coil alignment system (ACAS) for electric vehicles (EVs) with dynamic wireless charging (DWC) to mitigate the reduction in received power caused by lateral misalignment between the source and load coils. The key component of the ACAS is a novel sensor coil design, which can detect the load coil's left or right position relative to the source coil by observing the change in voltage phase. This allows the lateral misalignment to be estimated through the wireless power transfer (WPT) system alone, which is a novel tracking method for vehicular applications. Once misalignment is detected, the vehicle's lateral position is self-adjusted by an autonomous steering function. The feasibility of the overall operation of the ACAS was verified through simulation and experiments. In addition, an analysis based on experimental results was conducted, demonstrating that 26\% more energy can be transferred during DWC with the ACAS, just by keeping the vehicle's load coil aligned with the source coil.},
  number   = {3},
  urldate  = {2018-09-18},
  journal  = {Energies (19961073)},
  author   = {{Karam Hwang} and {Jaeyong Cho} and {Dongwook Kim} and {Jaehyoung Park} and {Jong Hwa Kwon} and {Sang Il Kwak} and {Hyun Ho Park} and {Seungyoung Ahn}},
  month    = mar,
  year     = {2017},
  keywords = {coil misalignment, COMPUTER simulation, dynamic wireless charging (DWC), ELECTRIC coils, ELECTRIC potential, electric vehicle (EV), ELECTRIC vehicles, magnetic sensing, power degradation, wireless power transfer (WPT), WIRELESS power transmission},
  pages    = {1--20},
  file     = {EBSCO Full Text:files/446/Karam Hwang et al. - 2017 - An Autonomous Coil Alignment System for the Dynami.pdf:application/pdf}
}

@article{lukic_cutting_2013,
